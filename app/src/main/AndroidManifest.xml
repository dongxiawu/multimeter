<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="personal.dongxia.android.multimeter">

    <application
        android:name=".MultimeterApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:enabled="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="personal.dongxia.android.multimeter.SecondActivity"
            android:taskAffinity="com.dongxia.test.launcher" />

        <activity android:name=".ip.IpActivity" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity-alias
            android:name="personal.dongxia.android.multimeter.SecondLauncherActivity"
            android:enabled="true"
            android:exported="true"
            android:targetActivity="personal.dongxia.android.multimeter.SecondActivity"
            android:taskAffinity="com.dongxia.test.launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>
        <meta-data
            android:name="UCSDKAppKey"
            android:value="${UCSDKAppKey}" />

        <provider
            android:name=".TestContentProvider"
            android:authorities="personal.dongxia.provider.test"
            android:enabled="true"
            android:exported="false"/>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>